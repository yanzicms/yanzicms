{
    "name": "yanzicms/yanzicms",
    "type": "project",
    "description": "Open Source Content Management. Yanzicms is the mobile-ready and user-friendly way to build your website.",
    "keywords": [
        "cms",
        "yanzi",
        "yanzicms"
    ],
    "license": "MIT",
    "homepage": "http://www.yanzicms.com",
    "support": {
        "issues": "https://github.com/yanzicms/yanzicms/issues",
        "source": "https://github.com/yanzicms/yanzicms"
    },
    "require": {
        "php": ">=5.6.0",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-curl": "*",
        "swuuws/framework": ">=3.5.0"
    },
    "require-dev": {
        "swuuws/framework": ">=3.5.0@dev"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/controller/",
            "model\\": "app/model/",
            "extend\\": "app/extend/",
            "plugin\\": "app/plugin/",
            "template\\": "public/template/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
